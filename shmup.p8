--update
function update_game()
	timer+=1
	
	if(pl.lives==0) then
	 timer=0
	 state=2
 end
	pl.vel_x=0
	pl.vel_y=0
	
	if(btn(0)) pl.vel_x=-pl.speed
	if(btn(1)) pl.vel_x=pl.speed
	if(btn(2)) pl.vel_y=-pl.speed
	if(btn(3)) pl.vel_y=pl.speed
	if(btnp(5)) then
	 add(pl.bullets,add_bullet(pl.x,pl.y,true,0.25,5,8))
	 pl.muzzle=2
	 end
	if(btnp(4) and pl.bombs>0) then
		pl.bombs+=-1
		enemy_bullets= {}
	end
	movement()
	pl.shipspr = 0
	if(btn(0) or btn(1)) pl.shipspr = 1
	if(pl.x>=120) pl.x=120
	if(pl.x<0) pl.x=0
	if((timer%20)==0) then
  add(suiciders,add_sui(rnd(128)))

	end 
	--
	---[[
	if(pl.y>120) pl.y=120
	if(pl.y<0) pl.y=0
	--]]
	pl.flame=((pl.flame+1)%7)+4
	foreach(enemies,enemy_behaviour)
	foreach(suiciders,suihaviour)
	hit_box()
end

