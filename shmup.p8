--renderers


function draw_bullet(o)
	if(not (o.y>=0 and o.y<=128 and o.x>=0 and o.x<=128)) then
		del(pl.bullets,o)
		del(enemy_bullets,o)
	end
	spr(o.bspr,o.x,o.y)
	if(o.bspr != 18 and o.bspr !=32) o.bspr+=1
end

function render_enemies(o)
	spr(21,o.x,o.y)
end



function render_starfield(o)
	pset(o.x,o.y,o.col)
	if(o.vely==4) then
		line(o.x,o.y-6,o.x,o.y-1,6)
	end
	if(o.y>=128) then 		
		del(stars,o)
		local star =createstar()
		star.y=0
		add(stars,star)
	end
end

function render_p(o)
 if(o.timer==0) del(particles,o)
	o.timer-=1
	circfill(o.x,o.y,o.radius,o.col)
end


function draw4(sp,where,x,y,p,f1,f2)
 local f1=f1 or 4
 local f2=f2 or 4
 local p=p or 1111
 local xm = (where%2)==1
 local ym = (flr(where/2)==1)
	local xp=(sp%16)*8+4*(where%2)
	local yp=(sp\16)*8+4*flr(where/2)
	if(p%10000>=1000) sspr(xp,yp,f1,f2,x,y,f1,f2,xm,ym)
	if(p%1000>=100) sspr(xp,yp,f1,f2,x+4,y,f1,f2,not xm,ym)
	if(p%100>=10) sspr(xp,yp,f1,f2,x,y+4,f1,f2,xm,not ym)
	if(p%10>=1) sspr(xp,yp,f1,f2,x+4,y+4,f1,f2,not xm, not ym)
end 	


function render_trail(o)
 --print("hello")
 if(o.timer==0) then
  del(trail,o)
  add(trail,add_trail(pl.x+4,pl.y+9))
 end
	o.timer-=1
	pset(o.x,o.y,o.col)
end


function render_sui(o)
	spr(o.sp,o.x,o.y)
	
end
